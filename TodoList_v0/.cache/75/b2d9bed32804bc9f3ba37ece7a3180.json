lumn":0},"source":"src/InfosOfTask.jsx","original":{"line":53,"column":20}},{"generated":{"line":118,"column":33},"source":"src/InfosOfTask.jsx","original":{"line":52,"column":16}},{"generated":{"line":118,"column":34},"source":"src/InfosOfTask.jsx","original":{"line":45,"column":12}},{"generated":{"line":118,"column":35},"source":"src/InfosOfTask.jsx","original":{"line":39,"column":8}},{"generated":{"line":118,"column":36},"source":"src/InfosOfTask.jsx","original":{"line":38,"column":11}},{"generated":{"line":118,"column":37},"source":"src/InfosOfTask.jsx","original":{"line":38,"column":4}},{"generated":{"line":119,"column":0},"source":"src/InfosOfTask.jsx","original":{"line":59,"column":1}},{"name":"InfosOfTask","generated":{"line":121,"column":15},"source":"src/InfosOfTask.jsx","original":{"line":61,"column":15}}],"sources":{"src/InfosOfTask.jsx":"import React, {useState, useCallback} from 'react'\n\nfunction InfosOfTask ({tache, handleClose, handleModify}) {\n\n    // On défini deux états pour suivre les valeurs entré dans les inputs\n    const [newTitle, setNewtitle] = useState(tache.title)\n    const [newPriority, setNewPriority] = useState(tache.priority)\n    const [ModifsValidated ,setModifsValidated] = useState(true)\n\n    function handleNewtitle(e) {\n        setNewtitle(e.target.value)\n        IsModifsValid();\n    }\n\n    function handleNewPriority (e) {\n        setNewPriority(e.target.value)\n        IsModifsValid();\n    }\n\n    function handleSubmit (e) {\n        handleModify(tache.id, newPriority, newTitle)\n    }\n\n    // fonction qui va appeler le callback pour fermer faire disparaitre la InfosOfTask\n    function handleClickToClose (e) {\n        handleClose({consult: false, id: null})\n    }\n\n    function IsModifsValid() {\n        if ((newPriority <= 10) && (newPriority >= 0) && (newTitle)) {\n            console.log(\"ok\");\n            setModifsValidated(false)\n        } else {\n            setModifsValidated(true)\n        }\n    }\n\n    return <div className=\"card\">\n        <div className=\"card-body\">\n            <div className=\"d-flex justify-content-between align-items-center\">\n                <h5 className=\"card-title mr-2\">{tache.title}</h5>\n                <button onClick={handleClickToClose} className=\"badge badge-danger badge-pill mr-1\">X</button>\n            </div>\n            <hr/>\n            <div className=\"form-group\">\n                <label htmlFor=\"title\">Titre de la tâche</label>\n                <input className=\"form-control mb-2\" type=\"text\" id=\"title\" name=\"title\" value={newTitle} onChange={handleNewtitle}/>\n\n                <label htmlFor=\"priority\">Priorité</label>\n                <input className=\"form-control\" type=\"number\" min=\"0\" max=\"10\" name=\"priority\" id=\"priority\" placeholder=\"Une valeur entre 0 et 10\" value={newPriority} onChange={handleNewPriority}/>\n\n                <div className=\"text-center mt-4\">\n                    <button className=\"btn btn-success\" onClick={handleSubmit} disabled={ModifsValidated}>Valider les modifications</button>\n                </div>\n            </div>\n        </div>\n    </div>\n\n}\n\nexport default InfosOfTask"},"lineCount":null}},"error":null,"hash":"16d127614e37b24987a606c87f110c4b","cacheData":{"env":{}}}